# Example environment variables for GPU Charts web application

# API Configuration
# Override the default API base URL (default: https://api.rednax.io)
REACT_APP_API_BASE_URL=https://api.rednax.io

# Development overrides
# For local development, uncomment and modify:
# REACT_APP_API_BASE_URL=https://localhost:8443

# Debug Configuration
# Enable debug mode for additional logging
REACT_APP_ENABLE_DEBUG=false

# Performance Monitoring
# Enable performance monitoring (default: true)
REACT_APP_PERFORMANCE_MONITORING=true

# Build Configuration
# Environment mode (automatically set by build tools)
NODE_ENV=production

# Architecture Configuration
# Use new architecture (data-manager, renderer, wasm-bridge) instead of legacy charting
REACT_APP_USE_NEW_ARCHITECTURE=false