{
  "name": "graph-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test && npm run test:e2e",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:visual": "cypress run --spec \"cypress/e2e/visual-regression-*.cy.ts\"",
    "cy:visual:ci": "cypress run --config-file cypress.config.ci.cjs --spec \"cypress/e2e/visual-regression-*.cy.ts\"",
    "cy:visual:update": "cypress run --spec \"cypress/e2e/visual-regression-*.cy.ts\" --env visualRegressionType=base",
    "cy:visual:update:script": "node scripts/update-visual-baselines.js",
    "cy:visual:presets": "cypress run --spec \"cypress/e2e/visual-regression-presets.cy.ts\"",
    "cy:visual:interactions": "cypress run --spec \"cypress/e2e/visual-regression-interactions.cy.ts\"",
    "cy:visual:viewports": "cypress run --spec \"cypress/e2e/visual-regression-viewports.cy.ts\"",
    "cy:visual:metrics": "cypress run --spec \"cypress/e2e/visual-regression-metrics.cy.ts\"",
    "cy:ema": "cypress run --spec \"cypress/e2e/ema-indicators.cy.ts\"",
    "cy:ema:open": "cypress open --e2e --browser chrome --spec \"cypress/e2e/ema-indicators.cy.ts\"",
    "analyze": "vite build --mode analyze",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.445.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.4",
    "cypress-image-diff-js": "^2.4.0",
    "cypress-visual-regression": "^5.3.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^16.2.0",
    "jsdom": "^25.0.1",
    "msw": "^2.10.5",
    "postcss": "^8.4.47",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.41.1",
    "@swc/core-win32-x64-msvc": "^1.11.31"
  }
}
