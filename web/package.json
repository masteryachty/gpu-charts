{
  "name": "graph-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wasm": "cd .. && wasm-pack build --target web --out-dir web/pkg --dev",
    "dev:watch": "cd .. && ./scripts/dev-build.sh",
    "dev:server": "cd ../server && cargo run --target x86_64-unknown-linux-gnu",
    "dev:server:build": "cd ../server && cargo build --target x86_64-unknown-linux-gnu",
    "setup:ssl": "cd .. && ./scripts/setup-ssl.sh",
    "dev:full": "concurrently \"npm run dev:watch\" \"npm run dev\"",
    "dev:complete": "concurrently \"npm run dev:watch\" \"npm run dev:server\" \"npm run dev\"",
    "build": "npm run build:wasm && tsc -b && vite build",
    "build:wasm": "cd .. && wasm-pack build --target web --out-dir web/pkg",
    "build:server": "cd ../server && cargo build --release --target x86_64-unknown-linux-gnu",
    "test:server": "cd ../server && cargo test --target x86_64-unknown-linux-gnu",
    "test:server:api": "cd ../server && ./test_api.sh",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:data": "playwright test data-visualization.spec.ts data-scenarios.spec.ts",
    "test:basic": "playwright test basic.spec.ts",
    "test:all": "playwright test --project=chromium"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.445.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^16.2.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^6.3.5",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
